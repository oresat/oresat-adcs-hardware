+ VALUE={if(v(drain,source)>0, 	(A1*(1-aITc*(Temp2-25))*log(1.0+exp((v(gate,source)-(k2*(1-k2Tc*(Temp-25))))/k3))*  	v(drain,source)/(1 + max(x0_0+x0_1*(1-x0_1_TC*(Temp2-25))*v(gate,source),0.2)*v(drain,source)) ),  	(-A1*(1-aITc*(Temp2-25))*log(1.0+exp((v(gate,drain)-(k2*(1-k2Tc*(Temp-25))))/k3))*  	v(source,drain)/(1 + max(x0_0+x0_1*(1-x0_1_TC*(Temp2-25))*v(gate,drain),0.2)*v(source,drain)) ) ) }
