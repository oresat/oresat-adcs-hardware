+ VALUE={0.5*ags2*ags4*log(1+exp((v(gate,drain)-ags3)/ags4))+ 	agd2*agd4*log(1+exp((v(gate,drain)-agd3)/agd4))+  	agd5*agd7*log(1+exp((v(gate,drain)-agd6)/agd7))+  	agd8*agd10*log(1+exp((v(gate,drain)-agd9)/agd10))+  	agd1*v(gate,drain) }
